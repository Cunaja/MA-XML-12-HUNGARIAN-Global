<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Suggestion">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="FocusingSuggestion">
        <item name="android:background">@drawable/highlighting_suggestion_background</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="CorpusName">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text2color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SuggestionText1">
        <item name="android:textSize">@dimen/suggestion_text1_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text1color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">@dimen/text_drawable_padding</item>
    </style>
    <style name="ThreeLineSuggestionText1">
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="DoubleLineSuggestionText1">
        <item name="android:textSize">@dimen/suggestion_text1_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text1color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="TileSuggestionText1">
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <style name="TileSuggestionText2">
        <item name="android:paddingTop">0dp</item>
    </style>
    <style name="GroupText">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/suggestion_group_name_color</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/card_top_height</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="recent_title_normal_style">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/suggestion_group_name_color</item>
    </style>
    <style name="recent_title_edit_style">
        <item name="android:textSize">@dimen/recent_edit_text_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text2color</item>
    </style>
    <style name="TipText">
        <item name="android:textSize">@dimen/tips_text_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/tips_text_color</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:background">@drawable/tips_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingStart">12.67dp</item>
        <item name="android:paddingEnd">12.67dp</item>
    </style>
    <style name="HotWordText">
        <item name="android:textSize">@dimen/hot_query_item_text_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/hot_query_item_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SearchHistoryText">
        <item name="android:textSize">@dimen/history_text_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text1color</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="RatingBar">
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
    </style>
    <style name="TileSuggestionRatingBar">
        <item name="android:layout_marginBottom">2.33dp</item>
    </style>
    <style name="SuggestionText2">
        <item name="android:textSize">@dimen/suggestion_text2_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text2color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SuggestionText3">
        <item name="android:textSize">@dimen/suggestion_text3_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/text2color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginEnd">5dp</item>
    </style>
    <style name="DoubleLineSuggestionText2">
        <item name="android:paddingTop">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ThreeLineSuggestionText2">
        <item name="android:layout_gravity">top|right|center_vertical|center_horizontal|center</item>
    </style>
    <style name="SuggestionDoubleLineText">
        <item name="android:textSize">@dimen/suggestion_text2_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">?android:attr/textColorSecondary</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
    </style>
    <style name="SuggestionIcon1">
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">@dimen/suggestion_icon_size</item>
        <item name="android:layout_height">@dimen/suggestion_icon_size</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>
    <style name="SuggestionMiddleIcon1">
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:layout_gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">@dimen/suggestion_icon_middle_size</item>
        <item name="android:layout_height">@dimen/suggestion_icon_middle_size</item>
        <item name="android:layout_marginEnd">13dp</item>
    </style>
    <style name="SuggestionOnlineIcon1">
    </style>
    <style name="WebSuggestionIcon1">
    </style>
    <style name="SuggestionIcon2">
        <item name="android:layout_width">@dimen/suggestion_icon_size</item>
        <item name="android:layout_height">@dimen/suggestion_icon_size</item>
        <item name="android:paddingEnd">@dimen/icon2_padding_right</item>
    </style>
    <style name="SuggestionText1.Query">
    </style>
    <style name="SuggestionText1.Suggested">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="SuggestionTileImage">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="SuggestionTileLinearLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TileSuggestionIcon">
        <item name="android:layout_width">@dimen/tile_width</item>
        <item name="android:layout_height">@dimen/tile_height</item>
        <item name="android:layout_marginBottom">6.66dp</item>
    </style>
    <style name="SixLineIcon">
        <item name="android:layout_width">@dimen/six_line_icon_width</item>
        <item name="android:layout_height">@dimen/six_line_icon_height</item>
        <item name="android:layout_marginTop">11.66dp</item>
        <item name="android:layout_marginBottom">6.66dp</item>
    </style>
    <style name="DoubleLineSuggestionHint">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ButtonMore">
        <item name="android:textSize">@dimen/more_text_size</item>
        <item name="android:textColor">@color/more_text_color</item>
        <item name="android:background">@drawable/btn_more</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/suggestion_more_height</item>
    </style>
    <style name="ClearHistory">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textColor">@color/more_text_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SearchInWeb">
        <item name="android:textSize">@dimen/web_query_text_size</item>
        <item name="android:textColor">@color/hot_query_item_color</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/history_item_height</item>
        <item name="android:drawablePadding">@dimen/search_in_web_left_drawable_padding</item>
        <item name="android:drawableStart">@drawable/icon_search</item>
        <item name="android:paddingStart">@dimen/suggestion_content_padding_side</item>
    </style>
    <style name="NetworkUnavailable">
        <item name="android:textSize">@dimen/network_unavailable_text_size</item>
        <item name="android:textColor">@color/network_unavailable_text_color</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="LabelTextStyle">
        <item name="android:textSize">@dimen/label_text_size</item>
        <item name="android:textColor">@color/label_text_color</item>
    </style>
    <style name="SuggestionRightTopText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">11.66dp</item>
        <item name="android:layout_marginEnd">6.66dp</item>
    </style>
    <style name="PromptCardButtonText">
        <item name="android:textSize">@dimen/prompt_card_button_size</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@color/prompt_card_positive_color</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">@drawable/card_bottom</item>
        <item name="android:padding">0dp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/prompt_card_button_height</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ActionButton">
        <item name="android:layout_width">@dimen/suggestion_action_button_width</item>
        <item name="android:layout_height">@dimen/suggestion_action_button_height</item>
    </style>
    <style name="Animation.SelectSearchSource">
        <item name="android:windowEnterAnimation">@anim/corpus_selector_open</item>
        <item name="android:windowExitAnimation">@anim/corpus_selector_close</item>
    </style>
    <style name="SearchActivity">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
        <item name="android:windowShowAnimation">@null</item>
        <item name="android:windowHideAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@anim/search_activity_in</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@anim/search_activity_in</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
        <item name="android:wallpaperOpenEnterAnimation">@null</item>
        <item name="android:wallpaperOpenExitAnimation">@null</item>
        <item name="android:wallpaperCloseEnterAnimation">@anim/search_activity_in</item>
        <item name="android:wallpaperCloseExitAnimation">@null</item>
        <item name="android:wallpaperIntraOpenEnterAnimation">@null</item>
        <item name="android:wallpaperIntraOpenExitAnimation">@null</item>
        <item name="android:wallpaperIntraCloseEnterAnimation">@null</item>
        <item name="android:wallpaperIntraCloseExitAnimation">@null</item>
    </style>
    <style name="PullToInputTextStyle">
        <item name="android:textSize">@dimen/pull_to_input_text_size</item>
        <item name="android:textColor">@color/pull_to_input_text_color</item>
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginStart">@dimen/pull_to_input_gap</item>
    </style>
    <style name="Theme.QuickSearchBox">
    </style>
    <style name="Theme.QuickSearchBox.Search">
        <item name="android:windowBackground">@color/search_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/SearchActivity</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowSoftInputMode">adjustUnspecified|stateUnchanged|stateHidden|stateAlwaysHidden</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.QuickSearchBox.SearchWidgetConfig">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="Theme.SelectSearchSource">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.SelectSearchSource</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustUnspecified|stateUnchanged|stateHidden|stateAlwaysHidden</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
</resources>
